{
  "name": "camt-parser",
  "version": "1.1.0",
  "description": "A TypeScript library for parsing bank statements in the SEPA CAMT (ISO 20022) format with human-readable interfaces",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "camt",
    "sepa",
    "iso20022",
    "bank",
    "statement",
    "parser",
    "typescript",
    "human-readable"
  ],
  "author": "Darko Mijic",
  "license": "MIT",
  "dependencies": {
    "camt-parser": "^1.1.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/xml2js": "^0.4.14",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darko-mijic/camt-parser.git"
  },
  "bugs": {
    "url": "https://github.com/darko-mijic/camt-parser/issues"
  },
  "homepage": "https://github.com/darko-mijic/camt-parser#readme",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
